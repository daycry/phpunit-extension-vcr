{
    "name": "daycry/phpunit-extension-vcr",
    "description": "A library that allows you to easily use the PHP-VCR library in your PHPUnit tests.",
    "keywords": ["phpunit", "php-vcr", "tests", "testing", "vcr", "unit"],
    "license": "MIT",
    "authors": [
      {
        "name": "Daycry"
      }
    ],
    "require": {
      "php": "^8.2",
      "php-vcr/php-vcr": "^1.7"
    },
    "require-dev": {
      "phpunit/phpunit": "^10.0",
      "squizlabs/php_codesniffer": "^3.10",
      "phpstan/phpstan": "^1.10"
    },
    "autoload": {
      "psr-4": {
        "Daycry\\PHPUnit\\Vcr\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Daycry\\PHPUnit\\Vcr\\Tests\\": "tests/"
      }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
  }